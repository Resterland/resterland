{
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		},
		{
			"type": "vcs",
			"url": "https://github.com/resterland/resterland.git"
		}
	],

	"name": "resterland/resterland",
	"type": "typo3-cms-extension",
	"description": "TYPO3 extension based on bootstrap-package of Bajamin Kott with some excellent ideas and samples taken from the pizpalue distribution of Roman BÃ¼chler. Tweaked to the needs of ResterLand WebAtelier.",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"TYPO3",
		"extension",
		"bootstrap",
		"responsive"
	],
	"homepage": "https://resterland.ch",
	"authors": [
		{
			"name": "Roland Fuhrer",
			"email": "roland@resterland.ch",
			"homepage": "https://resterland.ch",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.2",
		"helhum/typo3-secure-web": "^0.2",
		"typo3/cms-core": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-impexp": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-scheduler": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-adminpanel": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-beuser": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-form": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-redirects": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-reports": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-setup": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-sys-note": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-t3editor": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-tstemplate": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-viewpage": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-info": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-rte-ckeditor": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-filemetadata": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-indexed-search": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-seo": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-lowlevel": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-linkvalidator": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-felogin": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"typo3/cms-belog": "^9.5 || ~10.0.0 || 10.0.*@dev || 10.1.*@dev",
		"causal/image_autoresize": "^1.9",
		"bk2k/bootstrap-package": "^10.0 || ^11.0",
		"fluidtypo3/vhs": "^5.2",
		"gridelementsteam/gridelements": "^9.3",
		"bo/slickcarousel": "^3.0",
		"svewap/ws-flexslider": "^1.5"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Resterland\\Resterland\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Resterland\\Resterland\\Tests\\": "Tests"
		}
	},
	"replace": {
		"resterland/resterland": "self.version",
		"typo3-ter/resterland": "self.version"
	},
	"config": {
		"platform": {
			"php": "7.2"
		},
		"optimize-autoloader": true
	},
	"scripts": {
		"set-version": [
			"Resterland\\Resterland\\Composer\\VersionScript::setVersion"
		]
	},
	"extra": {
		"typo3/cms": {
			"root-dir": "private",
			"web-dir": "public"
		}
	}
}
