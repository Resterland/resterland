{
    "name": "resterland/resterland",
    "type": "typo3-cms-extension",
    "description": "This is the basic site package for TYPO3 projects created by Resterland.",
    "homepage": "https://resterland.org/",
    "license": [
        "GPL-2.0-or-later"
    ],
    "keywords": [
        "TYPO3 CMS",
        "bootstrap",
        "responsive"
    ],
    "require": {
		"typo3/cms-core": "^12.4 || ^13.4",
		"typo3/cms-rte-ckeditor": "^12.4 || ^13.4",
		"typo3/cms-fluid-styled-content": "^12.4 || ^13.4"
	},
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Resterland\\Resterland\\": "Classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "typo3/cms-*": true,
            "roave/security-advisories": true,
            "typo3/class-alias-loader": true
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "resterland"
        }
    }
}
