# Logo in Navbar
lib.navbar-brand {
    10 = IMAGE
    10.file = {$logo.file}
    10.file.height = {$logo.height}
    10.file.width = {$logo.width}
    10.params = class="img-responsive nav-logo"
    10.altText = {$logo.alt}
    10.titleText =
    10.emptyTitleHandling = useAlt
    10.typolink >
    10.imageLinkWrap = 1
    10.imageLinkWrap {
        enable = 1
        typolink.parameter = 1
        typolink.ATagParams = class="navbar-brand" title="home"
        typolink.target = _top
    }
}